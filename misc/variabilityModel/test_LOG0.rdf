        cns:ufz ;
    prov:wasDerivedFrom cns:dfClimateAreaCrops,
        cns:dfPrecipitation,
        cns:dfTemperature ;
    prov:wasGeneratedBy cns:merge12 .

cns:dfVariability a prov:Entity,
        geokur:Data ;
